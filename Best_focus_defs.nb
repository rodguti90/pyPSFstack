Cell[BoxData[{
 RowBox[{
  RowBox[{"abeW", "[", 
   RowBox[{"u_", ",", "nr_", ",", "\[Alpha]_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox["1", "nr"], " ", 
